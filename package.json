{
  "name": "larry",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter @larry/web dev",
    "dev:api": "pnpm --filter @larry/api dev",
    "dev:worker": "pnpm --filter @larry/worker dev",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^7",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "prisma": "^7.2.0"
  }
}
